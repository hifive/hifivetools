 * Dual licensed under the MIT and GPL licenses:
 * <a>http://www.opensource.org/licenses/mit-license.php</a>
 * <a>http://www.gnu.org/licenses/gpl.html</a>
 *
 * Thanks to Amir-Hossein Sobhi for some excellent contributions!
 */
]]></license>
			<library version="2.42">
				<site url="http://malsup.github.com/jquery.blockUI.js" />
			</library>
		</category>

		<category org="jquery-mobile" name="jQuery Mobile" path="${default.js.lib.path}/jqplugins/jqm" licenseUrl="http://jquery.org/license/">
			<info>
				<title>jQuery Mobile: Touch-Optimized Web Framework for Smartphones &amp; Tablets</title>
				<description><![CDATA[
A unified, HTML5-based user interface system for all popular
mobile device platforms, built on the rock-solid jQuery and
jQuery UI foundation. Its lightweight code is built with progressive
enhancement, and has a flexible, easily themeable design.
<a>http://jquerymobile.com</a>
				]]></description>
			</info>
			<library version="1.1.1">
				<site url="http://code.jquery.com/mobile/1.1.1/jquery.mobile-1.1.1.zip" file-pattern="jquery.mobile-1.1.1/images/*" extract-path="images" />
				<site url="http://code.jquery.com/mobile/1.1.1/jquery.mobile-1.1.1.zip" file-pattern="jquery.mobile-1.1.1/*.css" />
				<site url="http://code.jquery.com/mobile/1.1.1/jquery.mobile-1.1.1.zip" file-pattern="jquery.mobile-1.1.1/*.js" />
			</library>
		</category>

		<category org="syntaxhighlighter" name="SyntaxHighlighter" path="${default.js.lib.path}" licenseUrl="http://alexgorbatchev.com/SyntaxHighlighter/download/">
			<info>
				<title>Syntax Highlighter - Alex Gorbatchev</title>
				<description><![CDATA[
SyntaxHighlighter is a fully functional self-contained code syntax highlighter developed in JavaScript.
<a>http://alexgorbatchev.com/SyntaxHighlighter</a>
				]]></description>
			</info>
			<library version="3.0.83">
				<site url="http://alexgorbatchev.com/SyntaxHighlighter/download/download.php?sh_current" file-pattern="syntaxhighlighter_3.0.83/scripts/shCore.js" />
				<site url="http://alexgorbatchev.com/SyntaxHighlighter/download/download.php?sh_current" file-pattern="syntaxhighlighter_3.0.83/styles/shCore.css" />
				<site url="http://alexgorbatchev.com/SyntaxHighlighter/download/download.php?sh_current" file-pattern="syntaxhighlighter_3.0.83/styles/shThemeDefault.css" />
			</library>
		</category>

		<category org="modernizr" name="modernizr" path="${default.js.lib.path}" licenseUrl="http://modernizr.com/license/">
			<info>
				<title>Modernizr: the feature detection library for HTML5/CSS3</title>
				<description><![CDATA[
Modernizr is a JavaScript library that detects HTML5 and CSS3 features in the user’s browser.
<a>http://modernizr.com</a>
				]]></description>
			</info>
			<library version="latest">
				<site url="http://modernizr.com/downloads/modernizr.js" />
			</library>
		</category>

		<category org="siliconforks" name="JSCoverage" path="jsTool/jscoverage" licenseUrl="http://siliconforks.com/jscoverage/license.html">
			<info>
				<title>JSCoverage - code coverage for JavaScript</title>
				<description><![CDATA[
JSCoverage is a tool that measures code coverage for JavaScript programs.
<a>http://siliconforks.com/jscoverage</a>
				]]></description>
			</info>
			<library version="0.5.1">
				<site url="http://siliconforks.com/jscoverage/download/jscoverage-0.5.1-windows.zip" file-pattern="jscoverage-0.5.1/*" />
			</library>
		</category>

	</site-libraries>


	<default-libraries>
		<library-ref org="hifive" name="hifive" version="1.0.4" />
		<library-ref org="jquery" name="jQuery" version="1.7" />
		<library-ref org="malsup" name="jQuery blockUI plugin" version="2.42" />
	</default-libraries>


	<base-projects>
		<base-project default-js-lib-path="WebContent/res/lib" url="file://nas/confidential/research/2012/(H24SXX)HTML5開発PF「hifive」による次世代Webアプリ開発/開発環境整備/site-resources/projects/wtp.zip">
			<info lang="ja">
				<title>hifive 基本構成 WTP プロジェクト</title>
				<description>[hifive利用時の基本構成]
				・JavaSriptプロジェクト
				・WebContentsがドキュメントルートの動的Webプロジェクト(Java環境)
				・Tomcatプラグインプロジェクト対応
				・サンプルのHTMLとしてhifiveのStep3を含む
				</description>
			</info>
			<replace>
				<file name="/.jslint"/>
				<file name="/.codeassist"/>
				<file name="/.tomcatplugin"/>
				<file name="/.settings/org.eclipse.wst.common.component"/>
			</replace>
			<natures>
				<nature id="org.eclipse.jdt.core.javanature" name="java" />
				<nature id="com.sysdeo.eclipse.tomcat.tomcatnature"  name="tomcat"/>
			</natures>
		</base-project>

		<base-project default-js-lib-path="src/main/webapp/res/lib" url="file://nas/confidential/research/2012/(H24SXX)HTML5開発PF「hifive」による次世代Webアプリ開発/開発環境整備/site-resources/projects/maven.zip">
			<info lang="ja">
				<title>hifive maven構成 プロジェクト</title>
				<description>[hifive利用時の基本構成]
				・JavaSriptプロジェクト
				・src/main/webappがドキュメントルートの動的Webプロジェクト(Java環境)
				・Tomcatプラグインプロジェクト対応
				・サンプルのHTMLとしてhifiveのStep3を含む
				</description>
			</info>
			<replace>
				<file name="/.jslint"/>
				<file name="/.codeassist"/>
				<file name="/.tomcatplugin"/>
				<file name="/.settings/org.eclipse.wst.common.component"/>
			</replace>
			<natures>
				<nature id="org.eclipse.jdt.core.javanature" name="java" />
				<nature id="com.sysdeo.eclipse.tomcat.tomcatnature"  name="tomcat"/>
			</natures>
		</base-project>

		<base-project default-js-lib-path="" url="file://nas/confidential/research/2012/(H24SXX)HTML5開発PF「hifive」による次世代Webアプリ開発/開発環境整備/site-resources/projects/simple.zip">
			<info lang="ja">
				<title>hifive 最小構成 プロジェクト</title>
				<description>[hifive利用時の最小構成]
				・JavaSriptプロジェクト
				・サンプルのHTMLとしてhifiveのStep3を含む
				</description>
			</info>
			<info>
				<title>hifive simple project</title>
				<description>hifive simple project</description>
			</info>
			<replace>
				<file name="/.jslint"/>
				<file name="/.codeassist"/>
			</replace>
		</base-project>
	</base-projects>

</libraries>

